SELECT
    ficha.pasta AS 'PASTA',
    anda.data_andamento AS 'DATA_ANDAMENTO_MAIS_RECENTE',
    anda.tipo_andamento AS 'TIPO_ANDAMENTO_MAIS_RECENTE',
	CASE 
        WHEN anda.tipo_andamento in (
					'CITAÇÃO - ÊXITO PARCIAL',
					'CITAÇÃO LEGADO - PARCIAL',
					'CITAÇÃO PARCIAL',
					'CITAÇÃO PARCIAL - CONTROLE INTERNO',
					'CITAÇÃO PARCIAL - DEVEDOR PRINCIPAL CITADO',
					'CITAÇÃO PARCIAL - DSO',
					'CITAÇÃO PARCIAL - ENDEREÇO VALIDADO',
					'CITAÇÃO PARCIAL - PJ',
					'CITAÇÃO PARCIAL DSO - LEGADO',
					'CITAÇÃO PARCIAL PJ - LEGADO',
					'CITAÇÃO PARCIAL PJ - LEGADO ',
					'CITAÇÃO POR EDITAL - PARCIAL',
					'CITAÇÃO POSITIVA - DEVEDOR PRINCIPAL CITADO',
					'IMP 023 - CITAÇÃO PARCIAL - PJ',
					'IMP 024 - CITAÇÃO PARCIAL - DSO',
					'IMP 030 - CITAÇÃO VIA WHATSAPP PARCIAL - PJ',
					'IMP 031 - CITAÇÃO VIA WHATSAPP PARCIAL - DSO') 
					
        THEN 'CITAÇÃO PARCIAL'
	WHEN anda.tipo_andamento in (
					'ACORDO FORMALIZADO',
					'CITAÇÃO',
					'CITAÇÃO - COMPARECIMENTO ESPONTANEO',
					'CITAÇÃO - CONTROLE INTERNO/ ESTRATÉGIA',
					'CITAÇÃO - ÊXITO TOTAL',
					'CITAÇÃO - LEGADO',
					'CITAÇÃO (H)',
					'CITAÇÃO / AUTOMOTIVOS-VEÍCULO - POSITIVO',
					'CITAÇÃO / JUNTADA',
					'CITAÇÃO / RECEBIMENTO',
					'CITAÇÃO EDITAL - LEGADO',
					'CITAÇÃO LEGADO',
					'CITAÇÃO LEGADO ',
					'CITAÇÃO POR EDITAL',
					'CITAÇÃO POR HORA CERTA',
					'CITAÇÃO POSITIVA',
					'CITAÇÃO POSITIVA - CONTROLE INTERNO',
					'CITAÇÃO POSITIVA - ENDEREÇO VALIDADO',
					'CITAÇÃO POSITIVA - LEGADO',
					'CITAÇÃO VIA WHATSAPP POSITIVA – REALIZADA',
					'IMP 025 - CITAÇÃO  COMPARECIMENTO ESPONTANEO',
					'IMP 026 - CITAÇÃO POR EDITAL',
