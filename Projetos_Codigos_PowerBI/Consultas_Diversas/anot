SELECT
FP.PASTA,
FP.PRODUTO,
FP.STATUS_PASTA,
FP.PARTE_ADVERSA,
COMP.TIPO_COMPROMISSO,
COMP.DATA_LIMITE_COMPROMISSO,
COMP.DATA_CONCLUSAO_COMPROMISSO,
COMP.USUARIO_CONCLUSAO_COMPROMISSO

FROM JURIDICO.FICHAPROCESSUAL FP WITH(NOLOCK)
INNER JOIN JURIDICO.COMPROMISSOS_CONCLUIDOS COMP WITH(NOLOCK) ON FP.PASTA = COMP.PASTA
WHERE FP.PRODUTO IN ('ITAÚ - COBRANÇA ATACADO')
AND COMP.DATA_CONCLUSAO_COMPROMISSO >= '01/09/2024'

SELECT
FP.PASTA,
FP.PRODUTO,
FP.STATUS_PASTA,
FP.PARTE_ADVERSA,
ANDA.TIPO_ANDAMENTO,
ANDA.DATA_ANDAMENTO,
ANDA.HORA_ANDAMENTO,
ANDA.USUARIO_CADASTRO_ANDAMENTO

FROM JURIDICO.FICHAPROCESSUAL FP WITH(NOLOCK)
INNER JOIN JURIDICO.ANDAMENTOS ANDA WITH(NOLOCK) ON FP.PASTA = ANDA.PASTA
WHERE FP.PRODUTO IN ('ITAÚ - COBRANÇA ATACADO')
AND ANDA.DATA_ANDAMENTO >= '01/09/2024'