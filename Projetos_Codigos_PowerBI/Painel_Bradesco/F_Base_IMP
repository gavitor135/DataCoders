Select 
	fp.PASTA,
    fp.STATUS_PASTA,
	an.TIPO_ANDAMENTO,
	an.DATA_ANDAMENTO
	
FROM Juridico.FichaProcessual fp
left JOIN Juridico.Andamentos an WITH(NOLOCK) ON fp.PASTA = an.PASTA 
WHERE fp.PRODUTO = 'BRADESCO - COBRANÇA' 
